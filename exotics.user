      field[wpShort].value += #trait[attrBod]
      field[wpMedium].value += #trait[attrBod] * 2
      field[wpLong].value += #trait[attrBod] * 3
      field[wpExtreme].value += #trait[attrBod] * 4
      <after name="Calc trtFinal Final"/>
