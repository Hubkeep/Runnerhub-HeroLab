    <fieldval field="grEssence" value=".3"/>
    <arrayval field="usrArray" index="0" value="Wrist"/>
    <arrayval field="usrArray" index="1" value="Abdomen"/>
    <arrayval field="usrArray" index="2" value="Lower Back"/>
